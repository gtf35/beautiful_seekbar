<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="BeautifulSeekbar">

        <attr name="enable" format="boolean" /><!--启用-->
        <attr name="default_progress" format="integer" /><!--默认进度-->
        <attr name="outline_color" format="color" /><!--进度条边框颜色-->
        <attr name="inline_color" format="color" /><!--进度条进度颜色-->
        <attr name="indicator_circle_color" format="color" /><!--进度条手柄内部填充颜色-->
        <attr name="indicator_color" format="color" /><!--进度条手柄外圈颜色-->

    </declare-styleable>

</resources>
